{
  "manifest_version": 2,

  "name": "LnlyPpl",
  "description": "Connecting the lonely people of the world.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "<all_urls>"
  ]
}